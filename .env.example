# Copy this file to .env and fill in values. Do NOT commit your real secrets.

# Supabase (required)
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Optional, for server-side tasks (do NOT use in client-side apps)
SUPABASE_SERVICE_ROLE_KEY=

# SQLAlchemy database URL (optional if you only use session-state fallback)
# Example: postgresql+psycopg2://user:password@host:5432/dbname
DATABASE_URL=

# Google Gemini (required for LLM + embeddings)
GOOGLE_API_KEY=

# Mistral (required for meal planning in meal.py)
MISTRAL_API_KEY=

# Optional: Explicit timezone data location (rare)
# TZ=

# App telemetry / user agent (already defaulted in app)
# USER_AGENT=Nutrion/0.1 (https://github.com/zawlinnhtet03/nutrition-diet-assistant)
